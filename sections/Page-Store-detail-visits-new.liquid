{%- if metaobject.visit_our_store_detail.value.title != blank or metaobject.visit_our_store_detail.value.address != blank or metaobject.visit_our_store_detail.value.get_direction_map_url != blank or metaobject.visit_our_store_detail.value.opening_hours != blank or metaobject.visit_our_store_detail.value.call != blank or metaobject.visit_our_store_detail.value.email != blank or metaobject.visit_our_store_detail.value.thumbnail_video_or_image != blank -%}
  <div class="Page_store_detail_visits_main_new" style="background: {{ section.settings.main_background_color }};">
    {%- if metaobject.visit_our_store_detail.value.title != blank -%}
      <div class="page-width">
        <div class="Common_title_new Page_store_detail_visits_title_new">
          <h2>Visit our {{ metaobject.visit_our_store_detail.value.title }} store</h2>
        </div>
      </div>
    {%- endif -%}
    {%- if metaobject.visit_our_store_detail.value.title != blank or metaobject.visit_our_store_detail.value.address != blank or metaobject.visit_our_store_detail.value.get_direction_map_url != blank or metaobject.visit_our_store_detail.value.opening_hours != blank or metaobject.visit_our_store_detail.value.call != blank or metaobject.visit_our_store_detail.value.email != blank or metaobject.visit_our_store_detail.value.thumbnail_video_or_image != blank -%}
      <div class="Page_store_detail_visits_inner_new" style="background: {{ section.settings.inner_background_color }};">
        <div class="page-width">
          <div class="Page_store_detail_visits_leftrightboth_new">
            {%- if metaobject.visit_our_store_detail.value.title != blank or metaobject.visit_our_store_detail.value.address != blank or metaobject.visit_our_store_detail.value.get_direction_map_url != blank or metaobject.visit_our_store_detail.value.opening_hours != blank or metaobject.visit_our_store_detail.value.call != blank or metaobject.visit_our_store_detail.value.email != blank -%}
              <div class="Page_store_detail_visits_mainleft_new">
                <div class="Page_store_detail_visits_innerleft_new">
                  {%- if metaobject.visit_our_store_detail.value.title != blank or metaobject.visit_our_store_detail.value.address != blank or metaobject.visit_our_store_detail.value.get_direction_map_url != blank -%}
                    <div class="Page_store_detail_visits_innerleft_maintext_new">
                      <div class="Page_store_detail_visits_innerleft_innertext_new">
                        {%- if metaobject.visit_our_store_detail.value.title != blank or metaobject.visit_our_store_detail.value.get_direction_map_url != blank -%}
                          <div class="Page_store_detail_visits_innerleft_innertext_title_new">
                            {%- if metaobject.visit_our_store_detail.value.title != blank -%}
                              <h3>{{ metaobject.visit_our_store_detail.value.title }} store</h3>
                            {%- endif -%}
                            {%- if metaobject.visit_our_store_detail.value.get_direction_map_url != blank -%}
                              <div class="Page_store_detail_visits_innerleft_innertext_mobiledirection_new">
                                <a href="{{ metaobject.visit_our_store_detail.value.get_direction_map_url }}" target="_blank">
                                  <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M12.9532 5.07768L3.38689 7.94758L8.17029 9.86109L10.0835 14.9922L12.9532 5.07768Z" stroke="#0F0D1A" stroke-width="1.5" stroke-linejoin="round"/><circle cx="9" cy="9" r="8.3" stroke="#0F0D1A" stroke-width="1.4"/></svg><span>Get Direction</span>
                                </a>
                              </div>
                            {%- endif -%}
                          </div>
                        {%- endif -%}
                        {%- if metaobject.visit_our_store_detail.value.address != blank -%}
                          <div class="Page_store_detail_visits_innerleft_innertext_infos_new">
                            {{ metaobject.visit_our_store_detail.value.address }}
                          </div>
                        {%- endif -%}
                        {%- if metaobject.visit_our_store_detail.value.get_direction_map_url != blank -%}
                          <div class="Page_store_detail_visits_innerleft_innertext_direction_new">
                            <a href="{{ metaobject.visit_our_store_detail.value.get_direction_map_url }}" target="_blank">Get Direction</a>
                          </div>
                        {%- endif -%}
                      </div>
                    </div>
                  {%- endif -%}
                  {%- if metaobject.visit_our_store_detail.value.opening_hours != blank -%}
                    <div class="Page_store_detail_visits_innerleft_maintext_new">
                      <div class="Page_store_detail_visits_innerleft_innertext_new">
                        <div class="Page_store_detail_visits_innerleft_innertext_title_new">
                          <h3>Opening hours</h3>
                        </div>
                        <div class="Page_store_detail_visits_innerleft_innertext_infos_new">
                          {{ metaobject.visit_our_store_detail.value.opening_hours }}
                        </div>
                      </div>
                    </div>
                  {%- endif -%}
                  {%- if metaobject.visit_our_store_detail.value.call != blank or metaobject.visit_our_store_detail.value.email != blank -%}
                    <div class="Page_store_detail_visits_innerleft_maintext_new">
                      <div class="Page_store_detail_visits_innerleft_innertext_new">
                        <div class="Page_store_detail_visits_innerleft_innertext_title_new">
                          <h3>Contact</h3>
                        </div>
                        <div class="Page_store_detail_visits_innerleft_innertext_infos_new">
                          {%- if metaobject.visit_our_store_detail.value.call != blank -%}
                            Tel : {{ metaobject.visit_our_store_detail.value.call }}
                          {%- endif -%}
                          {%- if metaobject.visit_our_store_detail.value.email != blank -%}
                            <br>Email: {{ metaobject.visit_our_store_detail.value.email }}
                          {%- endif -%}
                        </div>
                      </div>
                    </div>
                  {%- endif -%}
                  {%- if metaobject.visit_our_store_detail.value.system.url != blank or metaobject.visit_our_store_detail.value.book_appointment_url != blank -%}
                    <div class="Page_store_detail_visits_innerleft_mainbothbutton_new">
                      <div class="Page_store_detail_objects_detail_2_innerleft_bothbutton_new">
                        {%- if metaobject.visit_our_store_detail.value.system.url != blank -%}
                          <div class="Page_store_detail_objects_detail_2_innerleft_fullbutton_new">
                            <a href="{{ metaobject.visit_our_store_detail.value.system.url }}" class="button button_white button_border" target="_blank">
                              VIEW DETAILS<svg width="22" height="22" viewBox="0 0 22 22" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M8 4L15 11L8 18" stroke="#0F0D1A" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg>
                            </a>
                          </div>  
                        {%- endif -%}
                        {%- if metaobject.visit_our_store_detail.value.book_appointment_url != blank -%}
                          <div class="Page_store_detail_objects_detail_2_innerleft_fullbutton_new">
                            <a href="{{ metaobject.visit_our_store_detail.value.book_appointment_url }}" class="button button_white button_border" target="_blank">
                              REGISTER NOW
                            </a>
                          </div>  
                        {%- endif -%}
                      </div>
                    </div>
                  {%- endif -%}
                </div>
              </div>
            {%- endif -%}
            {%- if metaobject.visit_our_store_detail.value.thumbnail_video_or_image != blank -%}
              <div class="Page_store_detail_visits_mainright_new">
                <div class="Page_store_detail_visits_innerright_new">
                  {%- if metaobject.visit_our_store_detail.value.thumbnail_video_or_image.sources[1].url contains '.mp4' or metaobject.visit_our_store_detail.value.thumbnail_video_or_image.sources[1].url contains '.MP4' -%}
                    <div class="Page_store_detail_visits_innerright_video_OR_image_new">
                      <video
                        id="Mp4Video-{{ forloop.index0 }}-inner"
                        class="video-div"
                        data-type="mp4"
                        src="{{ metaobject.visit_our_store_detail.value.thumbnail_video_or_image.sources[1].url }}"
                        loop muted playsinline autoplay>
                      </video>
                    </div>
                  {%- else -%}
                    <div class="Page_store_detail_visits_innerright_video_OR_image_new">
                      {{
                        metaobject.visit_our_store_detail.value.thumbnail_video_or_image | 
                        image_url: width: img.width | 
                        image_tag: widths: '180, 360, 540, 750, 900, 1080, 1296, 1512, 1728, 1944, 2048', 
                        fetchpriority: 'auto', 
                        class:'Hover_image_new',
                        alt: metaobject.visit_our_store_detail.value.thumbnail_video_or_image.alt
                      }}
                    </div>
                  {%- endif -%}
                </div>
              </div>
            {%- endif -%}
          </div>
        </div>
      </div>    
    {%- endif -%}
  </div>
{%- endif -%} 

{% schema %}
  {
    "name": "Page Store Detail Visits",
    "class": "Page_store_detail_visits_new",
    "settings": [
      {
        "type": "color",
        "id": "main_background_color",
        "label": "Add Main Background Color",
        "default": "#FFFFFF"
      },
      {
        "type": "color",
        "id": "inner_background_color",
        "label": "Add Inner Background Color",
        "default": "#F7F7F7"
      }
    ],
    "presets": [
      {
        "name": "Page Store Detail Visits"
      }
    ]
  }
{% endschema %}