
  <div class="Page_our_stores_objects_main_new" style="background: {{ section.settings.main_background_color }};">
    <div class="page-width">
      <div class="Page_our_stores_objects_inner_new">
        <div class="Page_our_stores_objects_detail_new">
          <div class="Page_our_stores_objects_detail_mainleft_new">
            <div class="Page_our_stores_objects_detail_innerleft_new">
              {%- for custom in shop.metaobjects.our_stores_page_list.values -%}
                {%- for rows in custom.our_stores_page_list.value -%}
                  {%- if rows.title.value != blank or rows.thumbnail_video_or_image.value != blank or rows.address.value != blank or rows.call.value != blank or rows.opening_hours.value != blank or rows.opening_hours_details.value != blank or rows.system.url != blank or rows.book_appointment_url.value != blank -%}
                    <div class="Page_our_stores_objects_detail_innerleft_mainboxs_new {% if rows.title.value == "Delhi South Ex 2" %}order-1 first-child{% endif %}{% if rows.title.value == "Mumbai" %}order-2{% endif %}{% if rows.title.value == "Hyderabad" %}order-3{% endif %}{% if rows.title.value == "Ahmedabad" %}order-4{% endif %}{% if rows.title.value == "Bengaluru" %}order-5{% endif %}{% if rows.title.value == "Surat" %}order-6{% endif %}{% if rows.title.value == "Indore" %}order-7{% endif %}{% if rows.title.value == "Delhi Pitampura" %}order-8 last-child{% endif %}">
                      <div class="Page_our_stores_objects_detail_innerleft_innerboxs_new">
                        {%- if rows.title.value != blank -%}
                          <div class="Page_our_stores_objects_detail_innerleft_innerboxs_titles_new">
                            <h2>KALKI {{ rows.title.value }}</h2>
                          </div>
                        {%- endif -%}
                        {%- if rows.thumbnail_video_or_image.value != blank -%}
                          <div class="Page_our_stores_objects_detail_innerleft_innerboxs_mainvideoimage_new">
                            <div class="Page_our_stores_objects_detail_innerleft_innerboxs_innervideoimage_new">
                              {%- if rows.see_virtual_tour_url.value != blank -%}
                                <div class="Page_our_stores_objects_detail_innerleft_innerboxs_tour_new">
                                  <a href="{{ rows.see_virtual_tour_url.value }}" class="button button_white">See Virtual tour</a>
                                </div>
                              {%- endif -%}
                              {%- if rows.thumbnail_video_or_image.value.sources[1].url contains '.mp4' or rows.thumbnail_video_or_image.value.sources[1].url contains '.MP4' -%}
                                <div class="Page_our_stores_objects_detail_innerleft_innerboxs_video_OR_image_new">
                                  <video
                                    id="Mp4Video-{{ forloop.index0 }}-inner"
                                    class="video-div"
                                    data-type="mp4"
                                    src="{{ rows.thumbnail_video_or_image.value.sources[1].url }}"
                                    loop muted playsinline autoplay>
                                  </video>
                                </div>
                              {%- else -%}
                                <div class="Page_our_stores_objects_detail_innerleft_innerboxs_video_OR_image_new">
                                  {{
                                    rows.thumbnail_video_or_image.value | 
                                    image_url: width: img.width | 
                                    image_tag: widths: '180, 360, 540, 750, 900, 1080, 1296, 1512, 1728, 1944, 2048', 
                                    fetchpriority: 'auto', 
                                    class:'Hover_image_new'
                                  }}
                                </div>
                              {%- endif -%}
                            </div>
                          </div>
                        {%- endif -%}
                        <div class="Page_our_stores_objects_detail_innerleft_innerboxs_details_main_new">
                          {%- if rows.address.value != blank -%}
                            <div class="Page_our_stores_objects_detail_innerleft_innerboxs_details_new">
                              {{ rows.address.value }}
                            </div>
                          {%- endif -%}
                          {%- if rows.call.value != blank -%}
                            <div class="Page_our_stores_objects_detail_innerleft_innerboxs_details_new">
                              Phone : {{ rows.call.value }}
                            </div>
                          {%- endif -%}
                          {%- if rows.opening_hours.value != blank -%}
                            <div class="Page_our_stores_objects_detail_innerleft_innerboxs_details_new">
                              Time : {{ rows.opening_hours.value }}
                            </div>
                          {%- endif -%}
                          {%- if rows.opening_hours_details.value != blank -%}
                            <div class="Page_our_stores_objects_detail_innerleft_innerboxs_details_new">
                              {{ rows.opening_hours_details.value }}
                            </div>
                          {%- endif -%}
                          {%- if rows.map_iframe.value != blank -%}
                            <div class="Page_our_stores_objects_detail_innerleft_innerboxs_direction_new">
                              <a href="javascript:void(0);" tab_id="{{ rows.title.value | handleize }}_map">
                                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M12.9532 5.0811L3.38689 7.951L8.17029 9.8645L10.0835 14.9956L12.9532 5.0811Z" stroke="#0F0D1A" stroke-width="1.5" stroke-linejoin="round"/><circle cx="9" cy="9" r="8.3" stroke="#0F0D1A" stroke-width="1.4"/></svg><span>Get Direction</span>
                              </a>
                            </div>
                          {%- endif -%}
                          {%- if rows.system.url != blank or rows.book_appointment_url.value != blank -%}
                            <div class="Page_our_stores_objects_detail_innerleft_innerboxs_bothbutton_new">
                              {%- if rows.system.url != blank -%}
                                <div class="Page_our_stores_objects_detail_innerleft_innerboxs_fullbutton_new">
                                  <a href="{{ rows.system.url }}" class="button button_white button_border">
                                    VIEW DETAILS  
                                  </a>
                                </div>  
                              {%- endif -%}
                              {%- if rows.book_appointment_url.value != blank -%}
                                <div class="Page_our_stores_objects_detail_innerleft_innerboxs_fullbutton_new">
                                  <a href="{{ rows.book_appointment_url.value }}" class="button button_white button_border">
                                    Book Appointment
                                  </a>
                                </div>  
                              {%- endif -%}
                            </div>
                          {%- endif -%}
                        </div>
                      </div>
                    </div>
                  {%- endif -%}
                {%- endfor -%}
              {%- endfor -%}
            </div>
          </div>
          <div class="Page_our_stores_objects_detail_mainright_new">
            <div class="Page_our_stores_objects_detail_innerright_new">
              {%- for custom in shop.metaobjects.our_stores_page_list.values -%}
                {%- for rows in custom.our_stores_page_list.value -%}
                    {%- if rows.map_iframe.value != blank -%}
                      <div class="Page_our_stores_objects_detail_innerright_mainboxs_new {{ rows.title.value | handleize }}_map_open" {% if forloop.first %} style="display:block;"{% endif %}>
                        <div class="Page_our_stores_objects_detail_innerright_innerboxs_new">
                          {{ rows.map_iframe.value }}
                        </div>
                      </div>
                    {%- endif -%}
                {%- endfor -%}
              {%- endfor -%}
            </div>
          </div>
          <div class="clearfix"></div>
        </div>
      </div>
    </div>
  </div>


{% schema %}
  {
    "name": "Page Our Stores Objects",
    "class": "Page_our_stores_objects_new",
    "settings": [
      {
        "type": "color",
        "id": "main_background_color",
        "label": "Add Main Background Color",
        "default": "#FFFFFF"
      }
    ],
    "presets": [
      {
        "name": "Page Our Stores Objects"
      }
    ]
  }
{% endschema %}