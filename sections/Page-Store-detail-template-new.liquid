<div class="Page_Store_detail_template_main_new" style="background: {{ section.settings.main_background_color }};">
  <div class="page-width">
    <div class="Page_Store_detail_template_inner_new">
      <div class="Page_Store_detail_template_breadcrumb_new">
        {% render 'Breadcrumb-navigation-new' %}
      </div>
    </div>
  </div>
</div>
{%- if metaobject.breadcrumb.value != blank -%} 
      {% for breadcrumb in metaobject.breadcrumb.value %}
            {{ breadcrumb }} 
      {% endfor %}
                             
  {%- else -%}
  {% for breadcrumb in metaobject.breadcrumb.value %}
      {{ page.title }} 
      {% endfor %}
  {%- endif -%}

{% schema %}
  {
    "name": "Store Detail Template",
    "class": "Page_Store_detail_template_new",
    "settings": [
      {
        "type": "color",
        "id": "main_background_color",
        "label": "Add Main Background Color",
        "default": "#FFFFFF"
      }
    ]
  }
{% endschema %}