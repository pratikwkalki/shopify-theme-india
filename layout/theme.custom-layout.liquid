      <script>
        document.addEventListener('click', (e) => {
          const priceFilterLabels = document.querySelectorAll('[js-price-filter-label]');
          priceFilterLabels?.forEach(priceBtn => {
            if(priceBtn?.contains(e.target)) {
              priceBtn?.querySelectorAll('input').forEach(input => {
                input?.click()
              });
            }
          })
        })
      </script>
