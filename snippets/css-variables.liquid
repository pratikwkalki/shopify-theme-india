{%- style -%}

  @font-face {
      font-family: 'Futura';
      src: url("{{ 'FuturaBook.woff2' | file_url }}") format('woff2');
      font-weight: 400;
      font-style: normal;
      font-display: swap;
  }
  @font-face {
      font-family: 'Futura';
      src: url("{{ 'FuturaMedium.woff2' | file_url }}") format('woff2');
      font-weight: 500;
      font-style: normal;
      font-display: swap;
  }
  @font-face {
      font-family: 'Futura';
      src: url("{{ 'FuturaHeavy.woff2' | file_url }}") format('woff2');
      font-weight: 900;
      font-style: normal;
      font-display: swap;
  }

  @font-face {
      font-family: 'Futura Now Headline';
      src: url("{{ 'futuranowheadline-webfont.woff2' | file_url }}") format('woff2'),
          url("{{ 'futuranowheadline-webfont.woff' | file_url }}") format('woff');
      font-weight: 400;
      font-style: normal;
      font-display: swap;
  }

  @font-face {
      font-family: 'Futura Now Headline';
      src: url("{{ 'futuranowheadlinebold-webfont.woff2' | file_url }}") format('woff2'),
          url("{{ 'futuranowheadlinebold-webfont.woff' | file_url }}") format('woff');
      font-weight: 700;
      font-style: normal;
      font-display: swap;
  }

  @font-face {
      font-family: 'Futura Now Headline';
      src: url("{{ 'futuranowheadlinemedium-webfont.woff2' | file_url }}") format('woff2'),
          url("{{ 'futuranowheadlinemedium-webfont.woff' | file_url }}") format('woff');
      font-weight: 500;
      font-style: normal;
      font-display: swap;
  }

  @font-face {
      font-family: 'Northwell';
      src: url("{{ 'northwell-webfont.woff2' | file_url }}") format('woff2'),
          url("{{ 'northwell-webfont.woff' | file_url }}") format('woff');
      font-weight: 400;
      font-style: normal;
      font-display: swap;
  }
  
{%- endstyle -%}