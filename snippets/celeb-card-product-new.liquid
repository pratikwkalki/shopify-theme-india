{{ collection.metafields.custom.celebrity_influencer_image.value }}
<img src="{{ collection.metafields.custom.celebrity_influencer_image.value | image_url: width: 610 }}" sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)"
                alt="{{ collection.metafields.custom.celebrity_influencer_image.alt | escape | default: celeb_card_product.title }}"
                class="motion-reduce"
                {% unless lazy_load == false %}
                  loading="lazy"
                {% endunless %}
                width="{{ collection.metafields.custom.celebrity_influencer_image.width }}"
                height="{{ collection.metafields.custom.celebrity_influencer_image.height }}"
              >